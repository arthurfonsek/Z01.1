function pow 2
    push argument 0
    pop local 0
    push argument 1
    pop local 1

    push argument 1
    push constant 1
    eq
if-goto end1
    push argument 1
    push constant 0
    eq
if-goto end0

label loop
    push local 0
    push argument 0
    call mult 2
    pop local 0
    push local 1
    push constant 1
    sub 
    pop local 1
    push local 1
    push constant 1
    eq 
if-goto end
goto loop
label end1
    push argument 0 
    return
label end0
    push constant 1
    return
label end
    push local 0
    return
